{
  "name": "osakamenesu-web",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "dev": "next dev --turbo -p 3000 --hostname 0.0.0.0",
    "build": "next build --webpack",
    "build:analyze": "ANALYZE=true next build --webpack",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "e2e:setup": "python ../../services/api/scripts/seed_admin_test_data.py",
    "test:e2e": "playwright test --reporter=line",
    "test:e2e:real": "bash ./scripts/run-e2e-real.sh",
    "test:e2e:visual": "cross-env E2E_ENABLE_VISUAL=1 playwright test e2e/visual-regression.spec.ts",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "screenshot:notifications": "node scripts/capture-notifications-screenshot.mjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "cross-env STORYBOOK_DISABLE_TELEMETRY=1 storybook build",
    "chromatic": "pnpm build-storybook && pnpm exec chromatic --storybook-dist-dir=storybook-static"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@sentry/node": "^10.32.1",
    "clsx": "2.1.1",
    "dayjs": "^1.11.19",
    "import-in-the-middle": "^2.0.1",
    "lucide-react": "^0.562.0",
    "next": "^16.1.1",
    "next-pwa": "^5.6.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-intersection-observer": "^10.0.0",
    "require-in-the-middle": "8.0.1",
    "tailwind-merge": "^3.4.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@next/bundle-analyzer": "^16.1.1",
    "@next/eslint-plugin-next": "^16.1.1",
    "@playwright/test": "^1.57.0",
    "@sentry/nextjs": "^10.32.1",
    "@storybook/addon-essentials": "^8.6.15",
    "@storybook/addon-interactions": "^8.6.15",
    "@storybook/addon-links": "^8.6.15",
    "@storybook/nextjs": "^8.6.15",
    "@storybook/react-webpack5": "^8.6.15",
    "@storybook/test": "^8.6.15",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "10.4.23",
    "babel-plugin-react-compiler": "^1.0.0",
    "chromatic": "^13.3.4",
    "critters": "^0.0.25",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.3.0",
    "playwright": "^1.57.0",
    "postcss": "8.5.6",
    "prettier": "^3.7.4",
    "storybook": "^8.6.15",
    "tailwindcss": "^4.1.18",
    "typescript": "5.9.3",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@10.20.0",
  "pnpm": {
    "overrides": {
      "glob": ">=10.5.0",
      "js-yaml": ">=4.1.1"
    }
  }
}
